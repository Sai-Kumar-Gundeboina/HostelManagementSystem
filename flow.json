        "func": "const jwt = global.get('jwt');\nconst secret = \"mySecretKey\";\ntry{\n    const token = msg.req.headers.authorization?.split(\" \")[1];\n    const decoded = jwt.verify(token, secret)\n    if (decoded.role==='admin'){\n        msg.topic = `\n        SELECT id, name, password, mobile, emailid, roomType, permanentAdr, status, allocatedRoom, registered_at \n        FROM tenants`;\n        return  msg\n    }\n}catch(e){\n    msg.statusCode = 403;\n    msg.payload = { error: \"Un authorized\"}\n    return msg;\n}\nreturn msg;",
